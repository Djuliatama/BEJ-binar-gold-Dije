class Citycar {
    constructor (model, tahun, fitur) {
        this.model = model; //model ini jd private variable kalau ditambah _
        this.tahun = tahun;
        this.fitur = fitur;
     }

     updateModel(inputModel) {
        this.model = inputModel;
     }

     #Introduce = () => {
        console.log (`Mobil toyota tipe city car model ${this._model}`);
        return "";
     }

     talk() {
        console.log(this.#Introduce());
     }

}
    
let Yaris = new CityCar ("Yaris Bakpao",2013,"Anti Lock Braking System");  
Yaris.talk()
